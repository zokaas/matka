{
  "kind": "collectionType",
  "collectionName": "questions_fis",
  "info": {
    "singularName": "questions-fi",
    "pluralName": "questions-fis",
    "displayName": "Questions FI",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "questionParameter": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "questionLabel": {
      "type": "text",
      "required": true
    },
    "componentType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Text",
        "Textarea",
        "Number",
        "RadioGroup",
        "Select",
        "MultiSelectDropdown",
        "BeneficialOwner"
      ]
    },
    "automaticAnalysis": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "automaticAnalysisType": {
      "type": "enumeration",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "automaticAnalysis"
            },
            true
          ]
        }
      },
      "required": true,
      "default": "String",
      "enum": [
        "String",
        "Number",
        "True/False"
      ]
    },
    "errorMessages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::error-message-fi.error-message-fi"
    },
    "placeholder": {
      "type": "text",
      "conditions": {
        "visible": {
          "!=": [
            {
              "var": "componentType"
            },
            "BeneficialOwner"
          ]
        }
      }
    },
    "options": {
      "type": "component",
      "conditions": {
        "visible": {
          "!=": [
            {
              "var": "componentType"
            },
            "BeneficialOwner"
          ]
        }
      },
      "component": "kyc.option-item",
      "repeatable": true
    },
    "dynamicField": {
      "type": "dynamiczone",
      "components": [
        "kyc.dependent-question-fi",
        "kyc.info",
        "kyc.country-options",
        "kyc.beneficial-owner"
      ]
    }
  }
}
