#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@react-router+serve@7.7.1_react-router@7.7.1_react-dom@19.1.1_react@19.1.1__react@19.1.1__typescript@5.6.2/node_modules/@react-router/serve/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@react-router+serve@7.7.1_react-router@7.7.1_react-dom@19.1.1_react@19.1.1__react@19.1.1__typescript@5.6.2/node_modules/@react-router/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@react-router+serve@7.7.1_react-router@7.7.1_react-dom@19.1.1_react@19.1.1__react@19.1.1__typescript@5.6.2/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@react-router+serve@7.7.1_react-router@7.7.1_react-dom@19.1.1_react@19.1.1__react@19.1.1__typescript@5.6.2/node_modules/@react-router/serve/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@react-router+serve@7.7.1_react-router@7.7.1_react-dom@19.1.1_react@19.1.1__react@19.1.1__typescript@5.6.2/node_modules/@react-router/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@react-router+serve@7.7.1_react-router@7.7.1_react-dom@19.1.1_react@19.1.1__react@19.1.1__typescript@5.6.2/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@react-router/serve/bin.js" "$@"
else
  exec node  "$basedir/../@react-router/serve/bin.js" "$@"
fi
