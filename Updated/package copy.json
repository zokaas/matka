{
    "name": "kyc",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "files": [
        "./build/client",
        "./build/server"
    ],
    "scripts": {
        "build": "react-router build",
        "dev": "export APP_ENV=local && react-router dev",
        "dev:docker": "pnpm build && pnpm start",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
        "start": "react-router-serve ./build/server/index.js",
        "start:development": "export APP_ENV=development && react-router-serve ./build/server/index.js",
        "start:mock": "export USE_MOCK_DATA=true && APP_ENV=development && react-router-serve ./build/server/index.js",
        "start:production": "export APP_ENV=production && react-router-serve ./build/server/index.js",
        "typecheck": "react-router typegen && tsc"
    },
    "dependencies": {
        "@awesome.me/kit-60ea292f8c": "1.0.7",
        "@fortawesome/fontawesome-svg-core": "6.7.2",
        "@fortawesome/free-solid-svg-icons": "6.7.2",
        "@fortawesome/react-fontawesome": "0.2.2",
        "@react-router/fs-routes": "7.7.1",
        "@react-router/node": "7.7.1",
        "@react-router/serve": "7.7.1",
        "axios": "1.7.7",
        "env-cmd": "10.1.0",
        "isbot": "5.1.17",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-router": "7.7.1",
        "ui-components": "workspace:*",
        "zod": "4.1.11"
    },
    "devDependencies": {
        "@eslint/js": "9.12.0",
        "@react-router/dev": "7.7.1",
